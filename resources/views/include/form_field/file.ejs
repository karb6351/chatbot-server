<div class="field ">
  <label class="label"><%- key.charAt(0).toUpperCase() + key.slice(1);  %></label>
  <div class="file-preview">
    <div class="file">
      <label class="file-label">
        <input class="file-input" type="file" name="<%= key %>">
        <span class="file-cta">
          <span class="file-icon">
            <i class="fas fa-upload"></i>
          </span>
          <span class="file-label">
            Choose a fileâ€¦
          </span>
        </span>
        <!-- <span class="file-name">
            
          </span> -->
      </label>
    </div>
    <% if (obj) { %>
    <div class="preview m-left-10">
      <a target="blank" href="<%= obj[key] %>" class="button is-info">View</a>
    </div>
    <% } %>
  </div>
</div>